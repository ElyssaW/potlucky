<a href="/offer/new" class="btn btn-primary">Make New Offer</a>

<% offers.forEach(offer => { %>
    <div class="well">
        <h4><a href="/offer/show/<%= offer.id %>"><%= offer.title %> by <%= offer.user.name %></a></h4>
        <%= offer.content %> at <%= offer.location.address %>
    </div>
<% }) %>